<?xml version="1.0" encoding="utf-8"?>
<resources>

	<declare-styleable name="ShopirollerBadgeView">
		<attr name="sBackgroundColor" format="color"/>
		<attr name="sTextColor" format="color"/>
		<attr name="sText" format="string"/>
	</declare-styleable>

	<declare-styleable name="ShopirollerTextView">
		<attr name="sColorType" format="enum" >
			<enum name="primary" value="7"/>
			<enum name="secondary" value="9"/>
			<enum name="dark" value="2"/>
			<enum name="header" value="3"/>
			<enum name="text" value="4"/>
			<enum name="paragraph" value="5"/>
		</attr>
		<attr name="sSizeType" format="enum" >
			<enum name="head_one" value="0"/>
			<enum name="head_two" value="1"/>
			<enum name="heading_one" value="2"/>
			<enum name="heading_two" value="3"/>
			<enum name="paragraph_one" value="4"/>
			<enum name="regular" value="5"/>
			<enum name="badge" value="6"/>
			<enum name="badgeLarge" value="7"/>
		</attr>
	</declare-styleable>

	<declare-styleable name="ShopirollerImageView">
		<attr name="sTintType" format="enum" >
			<enum name="primary" value="8"/>
			<enum name="secondary" value="1"/>
			<enum name="dark" value="2"/>
			<enum name="header" value="3"/>
			<enum name="text" value="4"/>
			<enum name="paragraph" value="5"/>
		</attr>
	</declare-styleable>

	<declare-styleable name="ShopirollerToolbar">
		<attr name="sToolbarIsTransparent" format="boolean"/>
	</declare-styleable>

	<declare-styleable name="ShopirollerButton">
		<attr name="sButtonBackgroundColor" format="color"/>
		<attr name="sButtonTextColor" format="color"/>
		<attr name="sButtonText" format="string"/>
		<attr name="sButtonIcon" format="reference"/>
		<attr name="sButtonHasRadius" format="boolean"/>
		<attr name="sButtonColorType" format="enum" >
			<enum name="primary" value="6"/>
		</attr>
	</declare-styleable>

	<declare-styleable name="ShopirollerEmptyView">
		<attr name="sBackgroundImageTintColor" format="color"/>
		<attr name="sBackgroundImageIsOval" format="boolean"/>
		<attr name="sTitle" format="string"/>
		<attr name="sDescription" format="string"/>
		<attr name="sEmptyIcon" format="reference"/>
		<attr name="sShowNoContent" format="boolean"/>
	</declare-styleable>

	<declare-styleable name="ShopirollerFloatingActionButton">
		<attr name="sFABColorType" format="enum" >
			<enum name="primary" value="6"/>
		</attr>
	</declare-styleable>

	<declare-styleable name="StatusViewScroller">
		<attr name="circleRadius" format="dimension" />
		<attr name="stepCount" format="integer" />
		<attr name="currentCount" format="integer" />
		<attr name="lineLength" format="dimension" />
		<attr name="circleColor" format="color"/>
		<attr name="circleStrokeColor" format="color"/>
		<attr name="lineColor" format="color"/>
		<attr name="textColorStatus" format="color"/>
		<attr name="textSizeStatus" format="dimension"/>
		<attr name="circleStrokeWidth" format="dimension"/>
		<attr name="lineWidth" format="dimension"/>
		<attr name="textColorLabels" format="color"/>
		<attr name="textSizeLabels" format="dimension"/>
		<attr name="circleColorType">
			<flag name="none" value="0" />
			<flag name="fill" value="1" />
			<flag name="stroke" value="2" />
			<flag name="fillStroke" value="3" />
		</attr>
		<attr name="circleColorIncomplete" format="color"/>
		<attr name="circleColorCurrent" format="color"/>
		<attr name="circleStrokeColorIncomplete" format="color"/>
		<attr name="circleStrokeColorCurrent" format="color"/>
		<attr name="lineColorIncomplete" format="color"/>
		<attr name="lineColorCurrent" format="color"/>
		<attr name="textColorLabelsIncomplete" format="color"/>
		<attr name="textColorLabelsCurrent" format="color"/>
		<attr name="textColorStatusIncomplete" format="color"/>
		<attr name="textColorStatusCurrent" format="color"/>
		<attr name="completeDrawable" format="reference"/>
		<attr name="currentDrawable" format="reference"/>
		<attr name="incompleteDrawable" format="reference"/>
		<attr name="drawLabels" format="boolean"/>
		<attr name="strictObeyLineLength" format="boolean"/>
		<attr name="lineGap" format="dimension"/>
		<attr name="minStatusAdjacentMargin" format="dimension"/>
		<attr name="statusTopMargin" format="dimension"/>
		<attr name="currentStepZoom" format="float"/>
		<attr name="alignStatusWithCurrent" format="boolean"/>
		<attr name="statusFont" format="reference"/>
		<attr name="labelFont" format="reference"/>
		<attr name="android:entries" />
	</declare-styleable>

		<declare-styleable name="MaterialSearchView">
			<attr name="searchBackground" format="integer" />
			<attr name="searchVoiceIcon" format="integer" />
			<attr name="searchCloseIcon" format="integer" />
			<attr name="searchBackIcon" format="integer" />
			<attr name="searchSuggestionIcon" format="integer" />
			<attr name="searchSuggestionBackground" format="integer" />
			<attr name="android:hint" />
			<attr name="android:textColor" />
			<attr name="android:textColorHint" />
			<attr name="android:inputType" />
		</declare-styleable>


	<declare-styleable name="MobirollerButton">
		<attr name="mButtonBackgroundColor" format="color"/>
		<attr name="mButtonTextColor" format="color"/>
		<attr name="mButtonText" format="string"/>
		<attr name="mButtonIcon" format="reference"/>
		<attr name="mButtonHasRadius" format="boolean"/>
		<attr name="mButtonColorType" format="enum" >
			<enum name="primary" value="6"/>
		</attr>
	</declare-styleable>

</resources>